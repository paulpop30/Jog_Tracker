<h2>New Time Entry</h2>
<%= render "form", time_entry: @time_entry %>

<% if current_user.admin? && @user != current_user %>
  <%= link_to "Back", user_time_entries_path(@user), class: "btn btn-secondary" %>
<% else %>
  <%= link_to "Back", time_entries_path, class: "btn btn-secondary" %>
<% end %>
